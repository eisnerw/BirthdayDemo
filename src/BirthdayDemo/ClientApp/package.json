{
  "name": "birthday-demo",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "description": "Description for BirthdayDemo",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch' npm:backend:start",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf dist/{src,target/}",
    "cleanup": "rimraf bin/aot && rimraf dist/*",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "serve": "npm run start --",
    "start": "ng serve --hmr",
    "start-tls": "npm run webapp:dev-ssl",
    "pretest": "npm run lint",
    "test": "ng test --coverage --log-heap-usage -w=2",
    "test:watch": "npm run test -- --watch",
    "watch": "concurrently npm:start npm:backend:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build --configuration development",
    "webapp:build:prod": "ng build --configuration production",
    "webapp:dev": "ng serve",
    "webapp:dev-ssl": "ng serve --ssl",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:test": "npm run test --"
  },
  "config": {
    "default_environment": "prod"
  },
  "dependencies": {
    "@angular/animations": "^16.2.3",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "16.2.2",
    "@angular/compiler": "16.2.2",
    "@angular/core": "16.2.2",
    "@angular/forms": "16.2.2",
    "@angular/localize": "16.2.2",
    "@angular/platform-browser": "16.2.2",
    "@angular/platform-browser-dynamic": "16.2.2",
    "@angular/router": "16.2.2",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@ng-bootstrap/ng-bootstrap": "15.1.1",
    "@popperjs/core": "2.11.6",
    "bootstrap": "5.1.3",
    "bootstrap-sass": "^3.4.3",
    "dayjs": "1.11.0",
    "ngx-infinite-scroll": "16.0.0",
    "ngx-webstorage": "12.0.0",
    "rxjs": "7.5.5",
    "swagger-ui-dist": "^5.4.2",
    "tslib": "2.3.1",
    "zone.js": "0.13.1",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primeng": "^14.1.2",
    "angular2-query-builder": "^0.6.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "16.0.1",
    "@angular-builders/jest": "13.0.3",
    "@angular-devkit/build-angular": "16.2.0",
    "@angular-eslint/eslint-plugin": "13.2.0",
    "@angular/cli": "16.2.0",
    "@angular/compiler-cli": "16.2.2",
    "@angular/service-worker": "16.2.2",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "browser-sync": "2.27.9",
    "browser-sync-webpack-plugin": "2.3.0",
    "concurrently": "7.1.0",
    "copy-webpack-plugin": "10.2.4",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-webpack-plugin": "3.1.1",
    "generator-jhipster": "7.8.1",
    "generator-jhipster-dotnetcore": "3.9.1",
    "jest": "29.6.4",
    "jest-date-mock": "1.0.8",
    "jest-junit": "13.1.0",
    "jest-preset-angular": "11.1.1",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "2.6.2",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.17",
    "rimraf": "3.0.2",
    "swagger-ui-dist": "4.10.3",
    "ts-jest": "29.1.1",
    "typescript": "4.9.5",
    "wait-on": "6.0.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-merge": "5.8.0",
    "webpack-notifier": "1.15.0"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "overrides": {
    "webpack": "5.71.0"
  }
}